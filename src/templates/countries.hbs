{{#each this}}
<h1 class='countrie-name'>{{name}}</h1>
<div class="countries-container">
    <ul class="countries-list">
        <li class='countries-list__item'><b>Capital:</b> {{capital}}</li>
        <li class='countries-list__item'><b>Population:</b> {{population}}</li>
        <li class='countries-list__item'><b>Languages:</b>
            <ul class='languages-list'>
                {{#each languages}}
                <li class='languages-list__item'>{{this.name}}</li>
                {{/each}}
            </ul>
        </li>
    </ul>
    <img src={{flag}} alt="" class='flag'>
</div>
{{/each}}